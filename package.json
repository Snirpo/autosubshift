  "author": {
    "name": "Snirpo"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@google-cloud/speech": "^3.6.0",
    "fast-glob": "^3.1.1",
    "lodash": "^4.17.15",
    "node-vad": "^1.1.4",
    "talisman": "^0.21.0",
    "winston": "^3.2.1",
    "yargs": "^15.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.26",
    "fs-extra": "^8.1.0",
    "rimraf": "^3.0.1",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  }
}
