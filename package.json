{
  "name": "autosubsync",
  "version": "0.2.6",
  "description": "Auto synchronize subtitles with video file",
  "main": "./index.js",
  "bin": "./cli.js",
  "scripts": {
    "build": "node build.js",
    "watch": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
