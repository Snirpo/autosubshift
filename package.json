  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@google-cloud/speech": "^2.1.1",
    "fast-glob": "^2.2.7",
    "lodash": "^4.17.15",
    "node-vad": "^1.1.3",
    "talisman": "^0.19.1",
    "winston": "^3.2.1",
    "yargs": "^12.0.5"
  },
  "devDependencies": {
    "@types/node": "^8.10.55",
    "fs-extra": "^7.0.1",
    "rimraf": "^2.7.1",
    "tslint": "^5.20.0",
    "typescript": "^2.4.2"
  }
}
